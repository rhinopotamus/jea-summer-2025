<diagram xmlns="https://prefigure.org"
  dimensions="(400,200)" margins="(50,30,25,25)">
  <definition>H(Y)=2.26*1.067^Y</definition> 
  <definition>C(Y)=10</definition>
  <coordinates bbox="(0,0,25,15)">
    <grid-axes
      spacings="((0,2.5,25),(0,2.5,15))"
      hticks= "(0,5,25)"
      hlabels="(0,5,25)"
      vticks= "(0,5,15)"
      vlabels="(0,5,15)"
      h-zero-label="yes"
      v-zero-label="yes">
        <xlabel><m>Y</m></xlabel>
        <ylabel><m>H</m></ylabel>
    </grid-axes>
    <graph function="H"/>
    <graph function="C" dash="3 3"/>
  </coordinates>
</diagram>